#include <stdio.h>
int main(){
    int choice;
    printf ("Enter any number from 1 to 5: ");
    scanf ("%d", &choice);

    switch (choice){
        case 1:
        printf ("Food item: Burger \nCost: Rs.130\n ");
        break;
        
        case 2:
        printf ("Food item: Pasta \nCost: Rs.100\n ");
        break;

        case 3:
        printf ("Food item: Sandwich \nCost: Rs.50\n ");
        break;

        case 4:
        printf ("Food item: French Fries \nCost: Rs.90\n ");
        break;

        case 5:
        printf ("Food item: Pizza \nCost: Rs.200\n ");
        break;

        default:
        printf ("not a valid number!\n");

    
    }
    return 0;
    
}
